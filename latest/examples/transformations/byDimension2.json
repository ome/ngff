{
    "coordinateSystems": [
        { "name": "in", "axes": [ {"name": "i"}, {"name": "j"}, {"name": "k"}, {"name": "l"}] },
        { "name": "out", "axes": [ {"name": "x"}, {"name": "y"}, {"name": "z"}  ] }
    ],
    "coordinateTransformations": [ 
        {
            "type": "byDimension",
            "input": "in",
            "output": "out",
            "transformations" : [
                { "type": "translation", "translation": [1, 3], "input": ["i", "k" ], "output": ["y", "x"]},
                { "type": "scale", "scale": [2.0], "input": ["j"], "output": ["z"]}
            ]
        }
    ]
}
